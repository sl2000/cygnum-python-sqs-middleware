[Unit]
Description=Gunicorn instance to serve cygnum
After=network.target

[Service]
User=www-data
Group=www-data
WorkingDirectory=/home/ubuntu/cygnum
Environment="PATH=/home/ubuntu/cygnum/env/bin"
ExecStart=/home/ubuntu/cygnum/env/bin/gunicorn --workers 3 --bind unix:cygnum.sock -m 007 wsgi:app

[Install]
WantedBy=multi-user.target
